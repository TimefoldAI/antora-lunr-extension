{
  "name": "@antora/lunr-extension",
  "version": "1.0.0-alpha.0",
  "description": "Generate a Lunr index from a catalog.",
  "license": "MPL-2.0",
  "author": "Guillaume Grossetie <g.grossetie@gmail.com>",
  "homepage": "https://antora.org",
  "repository": "https://gitlab.com/antora/antora-lunr-extension",
  "bugs": {
    "url": "https://gitlab.com/antora/antora-lunr-extension/-/issues"
  },
  "main": "lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./generate-index": "./lib/generate-index.js",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "coverage": "nyc _mocha",
    "lint": "eslint \"{lib,test,npm}/**/*.js\"",
    "test": "_mocha"
  },
  "files": [
    "lib/",
    "supplemental_ui"
  ],
  "keywords": [
    "antora",
    "antora-component",
    "antora-extension",
    "lunr"
  ],
  "dependencies": {
    "cheerio": "1.0.0-rc.10",
    "html-entities": "~2.3",
    "lunr": "2.3.8",
    "lunr-languages": "~1.9"
  },
  "devDependencies": {
    "@antora/site-generator": "3.0.0-rc.2",
    "chai": "~4.3",
    "dirty-chai": "~2.0",
    "eslint": "~7.32",
    "eslint-config-standard": "~16.0",
    "mocha": "~9.1",
    "nyc": "~15.1"
  },
  "nyc": {
    "cache": true,
    "cacheDir": "node_modules/.cache/nyc",
    "include": [
      "lib/*.js"
    ],
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "reportDir": "coverage-report"
  },
  "workspaces": [
    "."
  ]
}
